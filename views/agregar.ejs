<h1 class="display-4">Agregar lector</h1>
<form method="post" action="/lectores/create">
    <div class="form-group">
        <label for="id_lec">Lector ID</label>
        <input type="text" name="id_lec" class="form-control">
    </div>
    <div class="form-group">
        <label for="facultad">facultad</label>
        <input type="text" name="facultad" class="form-control"></textarea>
    </div>
    <div class="form-group">
        <label for="sala">sala</label>
        <input type="text" name="sala" class="form-control"></textarea>
    </div>
    <input type="submit" value="Enviar" class="btn btn-primary">
</form>
