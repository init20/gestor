<ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/lectores/lista">lectores</a></li>
        <li class="breadcrumb-item active">Editar</li>
    </ol>
    <h1 class="display-4">Editar Lector</h1>
    <form method="post" action="/lectores/actualizar/<%= lector.id %>">
      <div class="form-group">
          <label for="id_lec">ID Lector</label>
          <input type="text" name="id_lec" class="form-control" value="<%= lector.id_lec %>">
      </div>
        <div class="form-group">
            <label for="faculad">Facultad</label>
            <input type="text" name="facultad" class="form-control" value="<%= lector.facultad %>">
        </div>
        <div class="form-group">
            <label for="sala">Sala</label>
            <input type="text" name="sala" class="form-control" value="<%= lector.sala %>">
        </div>
        <input type="submit" value="Enviar" class="btn btn-primary">
    </form>
